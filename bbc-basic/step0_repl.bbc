REM Step 0 of mal in BBC BASIC

LIBRARY "malio_builtin"

PROCmalio_init

REPEAT
  line$ = FNmalio_input("user> ")
  PROCmalio_println(FNrep(line$))
UNTIL FNmalio_eof

END

DEF FNREAD(a$)
=a$

DEF FNEVAL(a$)
=a$

DEF FNPRINT(a$)
=a$

DEF FNrep(a$)
=FNPRINT(FNEVAL(FNREAD(a$)))

